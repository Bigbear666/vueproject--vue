* {
  margin: 0;
  padding: 0;
}
ul,
li {
  list-style: none;
}
body {
  width: 100%;
  position: static;
  overflow-x: hidden;
  font-size: 12px;
  font-family: 微软雅黑;
  background: #ffffff;
}
#app {
  width: 100%;
  margin: auto;
}
#app .X-header {
  width: 1200px;
  margin: 30px auto;
  height: 34px;
  line-height: 34px;
}
#app .X-header a {
  text-decoration: none;
  width: 140px;
  height: 34px;
  font-size: 32px;
  font-weight: bold;
  color: #ff5600;
  float: left;
}
#app .X-header input {
  outline: none;
  vertical-align: middle;
}
#app .X-header input.first {
  float: left;
  width: 30%;
  height: 33px;
  border: 2px solid #e4393c;
  margin-left: 350px;
}
#app .X-header input.sec {
  width: 10%;
  height: 37px;
  background: #e4393c;
  font-size: 20px;
  color: #ffffff;
  border: none;
  cursor: pointer;
}
#app .X-nav {
  width: 100%;
  height: 40px;
  background: #4f4f4f;
  margin-top: 24px;
}
#app .X-nav .nav_2 {
  width: 1200px;
  margin: auto;
}
#app .X-nav .nav_2 .assort {
  width: 200px;
  height: 40%;
  background: #ff680b;
  float: left;
}
#app .X-nav .nav_2 .assort h1 {
  color: #ffffff;
  font-size: 18px;
  line-height: 40px;
  text-align: center;
}
#app .X-nav .nav_2 .nav_1 {
  width: 800px;
  height: 40px;
  float: left;
}
#app .X-nav .nav_2 .nav_1 ul {
  list-style: none;
}
#app .X-nav .nav_2 .nav_1 ul li {
  float: left;
  width: 100px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  margin-right: 3.75px;
  font-size: 12px;
}
#app .X-nav .nav_2 .nav_1 ul li a {
  text-decoration: none;
  display: block;
  color: #ffffff;
}
#app .X-nav .nav_2 .nav_1 ul li:nth-child(1) a {
  background: #ffffff;
  color: #ff5600;
}
#app .X-nav .nav_2 .nav_1 ul li:hover a {
  display: block;
  background: #ffffff;
  color: #ff5600;
}
.main_banner {
  width: 1200px;
  height: 390px;
  margin: auto;
}
.fl {
  float: left;
}
.banner_left {
  width: 200px;
  height: 390px;
  background: #f5f5f5;
}
.banner_left ul li {
  height: 50px;
  line-height: 50px;
  margin-left: 50px;
  font-size: 15px;
}
.banner_left ul li a {
  color: #333;
  text-decoration: none;
  background: none;
}
.banner_left ul li:hover a {
  color: #ff5600;
}
.banner_right {
  width: 80%;
  height: 380px;
  margin-left: 10px;
  margin-top: 10px;
}
.banner_right img {
  width: 100%;
  height: 100%;
}
img {
  border: 0;
}
#r-chanpin {
  width: 1200px;
  height: 540px;
  margin: auto;
}
.r-chanpin {
  width: 1200px;
  height: 40px;
  border-bottom: 2px solid #CCCCCC;
}
.r-chanpin h2 {
  width: 70px;
  height: 40px;
  display: block;
  line-height: 40px;
  font-size: 14px;
  font-weight: bold;
  text-align: center;
  border-bottom: 2px solid #FE943C;
}
.r-bake-cookie {
  width: 1200px;
  height: 460px;
  margin-top: 20px;
}
.r-bake {
  width: 350px;
  height: 450px;
  float: left;
}
.r-bake-img {
  width: 350px;
  height: 350px;
  text-align: center;
  line-height: 350px;
  display: table-cell;
  float: left;
  border: 1px solid #CCCCCC;
}
.r-bake img {
  margin-top: 5px;
}
.r-bake-img img {
  vertical-align: middle;
}
.r-bake img {
  margin-top: 5px;
}
.r-cokkie {
  width: 816px;
  height: 450px;
  float: left;
  margin-left: 30px;
}
.r-cokkie-top {
  width: 816px;
  height: 40px;
  border-bottom: 1px solid #CCCCCC;
}
.r-cokkie-top h1 {
  width: 240px;
  height: 40px;
  display: block;
  line-height: 40px;
  font-size: 18px;
  font-weight: bold;
  text-align: center;
  color: #5D5D58;
}
.r-cokkie-p1 {
  width: 816px;
  height: 30px;
  float: left;
  line-height: 30px;
  font-size: 12px;
  color: #5D5D58;
}
s,
strike,
del {
  text-decoration: line-through;
}
.r-cokkie-p1 .r-p1-span1 {
  color: #CC3300;
  font-size: 22px;
  font-weight: bold;
}
.r-p1-span2,
.r-p1-span3 {
  width: 11px;
  height: 11px;
  line-height: 7px;
  text-align: center;
  font-size: 16px;
  display: inline-block;
  border: 1px solid #5D5D58;
  background: #F7F7F7;
  cursor: pointer;
}
#r-p1-input {
  width: 50px;
  height: 15px;
  border: 1px solid #5D5D58;
  text-align: center;
}
.r-cokkie-a1 {
  width: 130px;
  height: 40px;
  display: block;
  text-align: center;
  line-height: 40px;
  border: 1px solid #FF6600;
  background: #FFF0E5;
  color: #FF6600;
  font-size: 14px;
  float: left;
}
.r-cokkie-a {
  width: 816px;
  height: 45px;
  float: left;
  margin-top: 12px;
}
.r-cokkie-a2 {
  width: 130px;
  height: 40px;
  display: block;
  text-align: center;
  line-height: 40px;
  background: #FF6600;
  border: 1px solid #FF6600;
  color: white;
  font-size: 14px;
  float: left;
  margin-left: 15px;
}
.r-cokkie-foot {
  width: 716px;
  height: 45px;
  float: left;
  margin-top: 230px;
  margin-left: 130px;
  color: #5D5D58;
}
.r-cokkie-foot span {
  width: 48px;
  height: 24px;
  display: inline-block;
  text-align: center;
  line-height: 24px;
  margin-left: 20px;
  border: 1px solid #5D5D58;
  cursor: pointer;
}
.L_footpage {
  text-align: center;
  margin: 20px 0;
}
.L_footpage {
  text-align: center;
  margin: 20px 0;
  cursor: pointer;
}
.L_footpage span {
  display: inline-block;
  margin: 0 10px;
}
#r-footer {
  width: 100%;
  height: 144px;
  background: #F5F5F5;
}
.r-foot {
  width: 1200px;
  height: 144px;
  margin: auto;
}
.r-foot ul {
  width: 235px;
  height: 129px;
  margin-top: 10px;
  float: left;
  text-align: center;
}
#r-footer .r-foot ul li {
  width: #ffffff;
  margin-top: 12px;
  height: 15px;
  line-height: 0;
  color: #666666;
  font-size: 12px;
}
#r-footer .r-foot ul li:nth-child(1) {
  font-size: 15px;
}
.r-foot-list a {
  font-size: 15px;
}
#r-footer .r-foot ul li a {
  color: #333;
  text-decoration: none;
}
#r-footer .r-foot ul li a:hover {
  color: #ff7f00;
  text-decoration: none;
}
#r-foo {
  width: 100%;
  height: 62px;
  background: #4D4D4D;
  padding: 15px;
}
#r-foo .r-fo {
  width: 340px;
  height: 25px;
  margin: auto;
}
#r-foo .r-fo a {
  color: #DBDBDB;
  font-size: 14px;
  margin-left: 12px;
  text-decoration: none;
}
#r-foo .r-fo a:hover {
  color: #ff7f00;
}
#r-foo p {
  text-align: center;
  color: #DBDBDB;
  font-size: 12px;
}
#r-foo p em {
  font-style: normal;
}
